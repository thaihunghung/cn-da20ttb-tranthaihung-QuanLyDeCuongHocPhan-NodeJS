    function getKeyTaiLieuByKey(code) {
        if (code === 'giaotrinh') {
            return 'GT';
        } else if (code === 'TaiLieuThamKhao') {
            return 'TL';
        } else if (code === 'HocLieu') {
            return 'HL';
        } else {
            return null;
        }
    }
